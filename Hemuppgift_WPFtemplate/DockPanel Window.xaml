<Window x:Class="Hemuppgift_WPFtemplate.DockPanel_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="DockPanel_Window" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button x:Name="btnRight" Content="Right" Height="30" Width="80" Margin="5" HorizontalAlignment="Left" VerticalAlignment="Center" DockPanel.Dock="Right"/>
            <Button x:Name="btnLeft" Content="Left" Height="30" Width="80" Margin="5" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Button x:Name="btnBottom" Content="Bottom" Height="30" Width="80" Margin="5" DockPanel.Dock="Bottom" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            <Button x:Name="btnTop" Content="Top" Height="30" Width="80" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Center" DockPanel.Dock="Top"/>
            <Button x:Name="btnCenter" Content="Center" Height="30" Width="80" Margin="5" VerticalAlignment="Center" HorizontalAlignment="Center"/>

        </DockPanel>

    </Grid>
</Window>
